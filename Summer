itsalpa = FALSE
isnum = FALSE
spcialchar = FALSE
valid = FALSE
s = 'btngana2@'

while valid == FALSE:
    for i in range(len(s)):
        if s[i].itsalpa():
            itsalpa = TRUE
        elif s[i].isnumeric():
            isnum = TRUE
        elif s[i].isalnum()==FALSE:
            spcialchar = TRUE
    
    
    if itsalpa and isnum and spcialchar:
        print('valid pass')
    else:
        print('invalid')
        valid =FALSE

