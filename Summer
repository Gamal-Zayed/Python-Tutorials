def myfunc1(x):
    s=0
    for i in range(len(x)):
        s = s+ int(x[i])
    #print(s)
    return s
    
x= input("enter list:").split()
p = myfunc1(x)
print(p)
#######################################################
def tempconverter(T):
    C = (T-32)*(5/9)
    k = C + 273.15
    #print("in Kelvins:", k, "In Celicius", C)
    return k, C
    
print(tempconverter(30))


