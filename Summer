def GeneralAngle(B):
    if float(B)<0:
        return 'Negative'
    elif 0<=float(B)<=360:
        return float(B)
    else:
        return int(B) % 360
	
angle = int(input('')
x = GeneralAngle(angle)
print(angletype(x))   
